{
  "name": "enroll",
  "version": "3.1.1",
  "description": "safer subscriptions",
  "main": "build/enroll.cjs.js",
  "module": "build/enroll.esm.js",
  "browser": "build/enroll.umd.js",
  "files": [
    "build"
  ],
  "scripts": {
    "clean": "rimraf build coverage",
    "prepare": "npm run lint && npm run clean && npm test",
    "build": "rollup -c",
    "pretest": "npm run build",
    "test": "jest",
    "test:development": "NODE_ENV=development jest",
    "develop": "npm run test:development -- --watch",
    "coverage": "npm run test:development -- --coverage",
    "lint": "eslint --fix . && echo 'Done linting.'"
  },
  "keywords": [
    "subscriptions",
    "subscribe",
    "publish",
    "broadcast",
    "listener",
    "listeners",
    "emit"
  ],
  "author": "rogerbf <roger@bergfa.lk> (http://bergfa.lk/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@babel/plugin-transform-modules-commonjs": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "babel-jest": "24.9.0",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.0",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "rollup": "1.21.2",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0"
  },
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "call-tree": "2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rogerbf/enroll.git"
  },
  "homepage": "https://github.com/rogerbf/enroll#readme"
